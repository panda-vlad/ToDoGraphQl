{
  "name": "geniusee",
  "type": "commonjs",
  "version": "1.0.0",
  "description": "Basic implementation for todo list",
  "main": "runner.js",
  "scripts": {
    "start": "pm2 start runner.js --name todo",
    "stop": "pm2 delete todo -s &> /dev/null",
    "test": "cross-env MODE=test jest --maxWorkers=1",
    "test:lint": "eslint lib tests --ext js",
    "nodemon": "MODE=development nodemon runner.js",
    "migration:test": "sequelize db:migrate --config lib/config.js --env test",
    "seed:dev": "sequelize db:seed:all --config lib/config.js --env development",
    "migration:dev": "sequelize db:migrate --config lib/config.js --env development",
    "migration": "sequelize db:migrate --config lib/config.js --env production"
  },
  "jest": {
    "modulePathIgnorePatterns": [
      "fixtures",
      "TestFactory.js"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Vlad1slavIP74/ToDoGraphQl.git"
  },
  "keywords": [
    "GraphQL"
  ],
  "pre-commit": [
    "test:lint"
  ],
  "author": "Vlad Skrygun",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Vlad1slavIP74/ToDoGraphQl/issues"
  },
